{
  "def": {
    "prefix": "def",
    "body": [
      "def $1 do",
      "  $2",
      "end"
    ]
  },
  "def inline": {
    "prefix": "defi",
    "body": [
      "def $1, do: $2"
    ]
  },
  "defp": {
    "prefix": "defp",
    "body": [
      "defp $1 do",
      "  $2",
      "end"
    ]
  },
  "defp inline": {
    "prefix": "defpi",
    "body": [
      "defp $1, do: $2"
    ]
  },
  "defmacro using": {
    "prefix": "defusing",
    "body": [
      "defmacro __using__(which) when is_atom(which) do",
      "  apply(__MODULE__, which, [])",
      "end"
    ]
  },
  "doc": {
    "prefix": "doc",
    "body": [
      "@doc \"\"\"",
      "$1",
      "\"\"\""
    ]
  },
  "doc examples": {
    "prefix": "docex",
    "body": [
      "## Examples",
      "",
      "    iex> $1"
    ]
  },
  "io inspect": {
    "prefix": "ioi",
    "body": [
      "IO.inspect "
    ]
  },
  "io inspect with chain": {
    "prefix": "cioi",
    "body": [
      "|> IO.inspect()"
    ]
  },
  "live view socket.assigns": {
    "prefix": "sa",
    "body": [
      "socket.assigns."
    ]
  },
  "live view assign inline": {
    "prefix": "asi",
    "body": [
      "|> assign($1)"
    ]
  },
  "live view handle_event": {
    "prefix": "he",
    "body": [
      "@impl true",
      "def handle_event(\"$1\", $2params, socket) do",
      "  $3",
      "end"
    ]
  },
  "live view handle_info": {
    "prefix": "hi",
    "body": [
      "@impl true",
      "def handle_info({:$1}, socket) do",
      "  $2",
      "end"
    ]
  }
}
